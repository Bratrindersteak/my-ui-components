{
  "name": "my-ui-components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/my-ui-components.umd.cjs",
  "module": "./dist/my-ui-components.js",
  "types": "./dist/my-ui-components.d.ts",
  "exports": {
    ".": {
      "types": "./dist/my-ui-components.d.ts",
      "import": "./dist/my-ui-components.js",
      "require": "./dist/my-ui-components.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dts": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.71.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}
